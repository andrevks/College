BeginFun

    showMeTheCode "Digite o num maximo da sequencia de fibo:".
	funny terms_to_calculate.
	grabInput terms_to_calculate.
	funny n1 <- 0.
	funny n2 <- 1.
	funny counted <- 0.
	funny new_number <- 0.


    showMeTheCode " ".
	funLoopWhile counted < terms_to_calculate do

        showMeTheCode n1.

		new_number <- n1 + n2.

        n1 <- n2.

		n2 <- new_number.

		counted <- counted + 1.


	endFunLoop.

EndFun
